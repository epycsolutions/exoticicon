{
  "name": "exoticicon",
  "version": "0.1.0-alpha.2",
  "description": "CLI based icon-font generation tool.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epycsolutions/exoticicon.git"
  },
  "keywords": [
    "icons",
    "fonts",
    "iconfonts",
    "svg",
    "vector"
  ],
  "author": "Epyc Solutions",
  "bugs": {
    "url": "https://github.com/epycsolutions/exoticicon/issues"
  },
  "homepage": "https://github.com/epycsolutions/exoticicon#readme",
  "main": "lib/index.js",
  "bin": {
    "exoticicon": "bin/exoticicon"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:complex": "jest --verbose",
    "build": "rimraf lib && tsc --project ./tsconfig.json",
    "watch": "npm run build -- --watch",
    "typecheck": "tsc --noEmit --project tsconfig.test.json --noEmit",
    "lint": "prettier --check ."
  },
  "files": [
    "bin/exoticicon",
    "lib/**/*.(js,ts,map)",
    "!lib/**/{__mocks__,__tests__}/",
    "templates/*.hbs"
  ],
  "prepublish": "build",
  "dependencies": {
    "change-case": "4.1.2",
    "cli-color": "2.0.3",
    "commander": "11.1.0",
    "glob": "8.0.3",
    "handlebars": "4.7.8",
    "slugify": "1.6.6",
    "svg2ttf": "6.0.3",
    "svgicons2svgfont": "12.0.0",
    "ttf2eot": "3.1.0",
    "ttf2woff": "3.0.0",
    "ttf2woff2": "5.0.0"
  },
  "devDependencies": {
    "@types/cli-color": "2.0.6",
    "@types/glob": "8.0.0",
    "@types/jest": "29.5.10",
    "@types/node": "20.10.3",
    "@types/svg2ttf": "5.0.3",
    "@types/svgicons2svgfont": "10.0.5",
    "@types/ttf2eot": "2.0.2",
    "@types/ttf2woff": "2.0.4",
    "@types/ttf2woff2": "2.0.2",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "semantic-release": "22.0.8",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "./src",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  }
}
